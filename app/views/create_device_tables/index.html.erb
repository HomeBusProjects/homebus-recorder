<p id="notice"><%= notice %></p>

<h1>Create Device Tables</h1>

<table>
  <thead>
    <tr>
      <th>Nickname</th>
      <th>Street address</th>
      <th>City</th>
      <th>State</th>
      <th>Postal code</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Api key</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @create_device_tables.each do |create_device_table| %>
      <tr>
        <td><%= create_device_table.nickname %></td>
        <td><%= create_device_table.street_address %></td>
        <td><%= create_device_table.city %></td>
        <td><%= create_device_table.state %></td>
        <td><%= create_device_table.postal_code %></td>
        <td><%= create_device_table.longitude %></td>
        <td><%= create_device_table.latitude %></td>
        <td><%= create_device_table.api_key %></td>
        <td><%= link_to 'Show', create_device_table %></td>
        <td><%= link_to 'Edit', edit_create_device_table_path(create_device_table) %></td>
        <td><%= link_to 'Destroy', create_device_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Create Device Table', new_create_device_table_path %>
